{% extends '::base.html.twig' %}

{% block body -%}

{% block menu %}
{% include 'FrontendVisitasBundle:Default:menu.html.twig' %}
{% endblock %}

    <h1>Registrar Salida</h1>

    <br><br>




    <table class="record_properties" border="1" cellspacing="9" cellpadding="5">
        <thead >
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Cedula</th>
                <th>Hora</th>
                <th>Fecha</th>

            </tr>
        </thead>
        <tbody>
            <tr>
                <td>{{ entity.id }}</td>
                <td>{{ entity.nombres }}</td>
                <td>{{ entity.apellidos }}</td>
                <td>{{ entity.cedula }}</td>
                <td>{{ hora }}</td>
                <td>{{ fecha }}</td>

            </tr>
        </tbody>
    </table>           



<br><br>


        <ul class="record_actions">
    <li><br>
        <a class="btn btn-success"   href="{{ path('control_visitas_usuario', { 'cedula':entity.cedula }) }}">Procesar</a>
        <br>
        <br>
        <!--a class="btn" href="{{ path('control_visitas_usuario') }}">Regresar</a-->
    </li>
</ul>
{% endblock %}
